import requests
import json
import pprint


response = {};
baseEndPoint = 'http://cloud.utradea.com/v1'

# AUTHORIZATION : UTRADEA
# plug in your Utradea API Key
# For more information on obtaining API Key visit: https://utradea.com/api-docs;

API_TOKEN_FROM_UTRADEA = ''

headers = {'Authorization': API_TOKEN_FROM_UTRADEA , 'Content-Type':'application/json'}
 
endpoints = {
    'trending': {
        'twitter': {
            'bullish': '/get-sentiment-trending/bullish?social=stocktwits&limit=200',
            'bearish': '/get-sentiment-trending/bearish?social=stocktwits&limit=200'
        },
        'stocktwits': {
            'bullish': '/get-sentiment-trending/bullish?social=twitter&limit=200',
            'bearish': '/get-sentiment-trending/bearish?social=twitter&limit=200'
        }
    },
    'socialMovingAverage': {
        'stocktwits': {
                '1m':  '/get-social-moving-averages/1m?tickers=AMZN,MSFT,TSLA,NVDA,NFLX,PLTR,GOOGL&social=stocktwits',
                '2w':  '/get-social-moving-averages/2w?tickers=AMZN,MSFT,TSLA,NVDA,NFLX,PLTR,GOOGL&social=stocktwits',
                '1w':  '/get-social-moving-averages/1w?tickers=AMZN,MSFT,TSLA,NVDA,NFLX,PLTR,GOOGL&social=stocktwits',
                '72h': '/get-social-moving-averages/72h?tickers=AMZN,MSFT,TSLA,NVDA,NFLX,PLTR,GOOGL&social=stocktwits',
        }, 
        'twitter': {
            '1m':  '/get-social-moving-averages/1m?tickers=AMZN,MSFT,TSLA,NVDA,NFLX,PLTR,GOOGL&social=twitter',
            '2w':  '/get-social-moving-averages/2w?tickers=AMZN,MSFT,TSLA,NVDA,NFLX,PLTR,GOOGL&social=twitter',
            '1w':  '/get-social-moving-averages/1w?tickers=AMZN,MSFT,TSLA,NVDA,NFLX,PLTR,GOOGL&social=twitter',
            '72h': '/get-social-moving-averages/72h?tickers=AMZN,MSFT,TSLA,NVDA,NFLX,PLTR,GOOGL&social=twitter',
        }

    }
}
 
# Can you pass a query parameter? Yes you can. please read the https://utradea.com/api-docs. For the purposes of this script it's not required.
try:
    res = requests.get(baseEndPoint + endpoints['socialMovingAverage']['twitter']['1m'], headers=headers);
    if res.status_code == 200:
        response = {
            'data': res.text,
            'statusCode': res.status_code 
        }
    else:
        response = {
        'data': res.text,
        'statusCode': res.status_code
    }
except Exception as e:
    print(e)
    response = {
        'data': None,
        'errorMessage': e, 
        'statusCode': res.status_code
    }
    
finally:
    pprint.pprint(response)
